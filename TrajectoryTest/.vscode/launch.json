{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "Launch PathWeaverTest",
      "request": "launch",
      "mainClass": "tests.PathWeaverTest",
      "projectName": "TrajectoryTest",
      "env": {
        "PATH": "$PATH:${workspaceFolder:TrajectoryTest}/build/jni/debug" 
      },
    },
    {
      "type": "java",
      "name": "Launch PlotTestClient",
      "request": "launch",
      "mainClass": "tests.PlotTestClient",
      "env": {
        "PATH": "$PATH:${workspaceFolder:TrajectoryTest}/build/jni/debug" 
      },
      "projectName": "TrajectoryTest"
    },
    {
      "type": "java",
      "name": "Launch PlotTestServer",
      "request": "launch",
      "mainClass": "tests.PlotTestServer",
      "env": {
        "PATH": "$PATH:${workspaceFolder:TrajectoryTest}/build/jni/debug" 
      },
      "projectName": "TrajectoryTest",
      "desktop": true
    },
    {
      "type": "java",
      "name": "CodeLens (Launch) - Pathfinder",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "console": "internalConsole",
      "stopOnEntry": false,
      "mainClass": "tests.Pathfinder",
      "vmArgs": [
        "-Djava.library.path=${workspaceFolder:TrajectoryTest}/libs"
      ],
      "projectName": "TrajectoryTest",
      "desktop": true
    },
    {
      "type": "java",
      "name": "Launch PlotTest",
      "request": "launch",
      "mainClass": "tests.PlotTest",
      "projectName": "TrajectoryTest"
    },
    {
      "type": "wpilib",
      "name": "WPILib Desktop Debug",
      "request": "launch",
      "desktop": true
    },
    {
      "type": "wpilib",
      "name": "WPILib roboRIO Debug",
      "request": "launch",
      "desktop": false
    }
  ]
}
